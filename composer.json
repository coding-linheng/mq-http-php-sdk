{
    "name": "coding-linheng/mq-http-php-sdk",
    "type": "library",
    "description": "Adapted hyperf component modified based on Alibaba Cloud SDK",
    "keywords": [
        "Aliyun",
        "Alicloud",
        "MQ",
        "Message Queue",
        "Message",
        "Queue",
        "Hyperf"
    ],
    "homepage": "https://github.com/aliyunmq/mq-http-php-sdk",
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": ">=6.0.0"
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    },
    "autoload": {
        "psr-4": {
            "MQ\\": "MQ/"
        }
    },
    "extra": {
        "hyperf": {
            "config": "MQ\\ConfigProvider"
        }
    }
}
